%%LIBMEMSYM=\\msbmcc03\ctm_av\PARM\RENT-DAT

%%DAT1=%%OYEAR.%%OMONTH

%%SOZ_DAT=%%$OYEAR.%%OMONTH

%%BEIBLATT=N


* Eingabe Directory 
%%INDIR=\\Msprintff01\source$\SOZ-Renten\%%$OYEAR.\DUE\%%DAT1.\KNAPP

* Drucker Freigabe
%%PRINTER=\\msdhp7\DLD5081319


%%PROT=KNAPP-PFDR0100-DF_FDRA-REST_%%SOZ_DAT..pdf 


%%INCLUDE=%%PROT

* erledigt Ordner
%%OUTDIR_PROCESSED=\\Msprintff01\source$\SOZ-Renten\%%$OYEAR.\DUE\%%DAT1.\KNAPP\gedruckt

* Datei Auswahl
%%EXCLUDE="-"

* Ausgabe Directory
%%FORCE_OUTDIR=N



